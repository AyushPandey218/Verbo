
{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/socket", "destination": "/api/socket" },
    { "source": "/socket.io/(.*)", "destination": "/api/socket" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
